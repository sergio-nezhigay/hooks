          </div>
        {% when 'newsletter' %}
        <style>
              .block-{{ block.id }} h3{
                font-size:{{ block.settings.desktop_heading_size }}px;
              }
              .block-{{ block.id }} p{
              font-size: {{ block.settings.desktop_body_size }}px;
              }
              @media screen and (max-width:756px){
                 .block-{{ block.id }} h3{
                font-size:{{ block.settings.mobile_heading_size }}px;
              }
                .block-{{ block.id }} p {
              font-size:{{ block.settings.mobile_body_size }}px;
              }
              }
            </style>
          <div class="footer-box block-{{ block.id }}">
            {% if block.settings.heading != blank %}
              <h3>
                <b>{{ block.settings.heading }}</b>
              </h3>
            {% endif %}
            <div class="footer-menu footer-news">
              {{ block.settings.content }}
              <div class="newsletter-form">
                {%- form 'customer', class: 'newsletter-form' -%}
                  <input
                    type="hidden"
                    name="contact[tags]"
                    value="newsletter"
                  >
                  <input
                    id="footerbtn"
                    class="newsletter-input"
                    type="text"
                    placeholder="Full Name:"
                    name="contact[name]"
                  >
                  <input
                    id="footerbtn"
                    class="newsletter-input"
                    type="text"
                    placeholder="Email:"
                    name="contact[email]"
                  >
                  <button
                    id="inputbtn"
                    type="submit"
                    class="button primary-btn btn-newsletter btn-new-secondry"
                  >
                    Subscribe
                  </button>
                  {%- if form.errors -%}
                    <small class="newsletter-form__message form__message" id="Newsletter-error--{{ section.id }}">
                      {%- render 'icon-error' -%}
                      {{- form.errors.translated_fields.email | capitalize }}
                      {{ form.errors.messages.email -}}
                    </small>
                  {%- endif -%}
                  {%- if form.posted_successfully? -%}
                    <h3
                      class="newsletter-form__message newsletter-form__message--success form__message"
                      id="Newsletter-success--{{ section.id }}"
                      tabindex="-1"
                      autofocus
                    >
                      Form Submitted.
                    </h3>
                  {%- endif -%}
                {%- endform -%}
              </div>
            </div>
          </div>
      {% endcase %}
    {% endfor %}
  </div>
</div>

