<style>
    .hp-footer-main {
      background-color: #09213a;
      padding: 100px;
    }

    .hp-footer-grid {
      display: flex;
      flex-direction: row;
      gap: 16px;

    }
    .footer-box {
      width: 25%;
    }
  .footer-logo svg{
    width:{{ section.settings.logo-size }}%;
    height:auto;
  }
  .footer-box h3, footer-box p{
    color:white;
  }
  .footer-menu-box h3{
    padding-left:40px;
  }
    .footer-description p {
      color: white;
      font-size: 18px;
    }
    .footer-menu {
      list-style: none;
    }

    .footer-menu li a {
      text-decoration: none;
      color: white;
      font-size: 24px;
      text-align: justify;
      font-weight: 400;
    }
    .footer-news p {
      color: white;
      font-size: 18px;
    }
    .footer-news .newsletter-form {
      display: flex;
      flex-direction: column;
      gap: 10px;
    }
    .footer-news .newsletter-form input {
      border-radius: 0 !important;
      background: transparent;
      border: none;
      border-bottom: 2px solid white;
      padding: 0 0 20px;
      margin-bottom: 10px;
      width: 100%;
      height: 55px;
      border-color: white;
      font-size: 21px;
      line-height: 28px;
      color: white;
    }
    .footer-news .newsletter-form .btn-newsletter {
      border-radius: 0 0 40px 0;
      font-size: 22px;
      font-weight: 500;
      min-width: 50%;
      max-width: 450px;
      width: 50%;
      height: 40px;
      line-height: 24px;
    }

    .stars{
      text-align:center;
    }
    .stars a{
     color:white;
    }
  .footer-affiliate{
    color:white;
    font-size:14px;
    margin:0;
  }
  .footer-text{
    color:white;
  }
    @media screen and (max-width: 756px) {
      .hp-footer-main {
        padding: 30px;
        padding-top:99px;
      }
      .hp-footer-grid {
        display: flex;
        flex-direction: column;
        gap: 16px;
      }
      .footer-box {
        width: 100%;
      }
       .footer-menu-box h3{
    padding-left:0px;
  }
      .footer-menu {
        padding: 0;
      }
    }
</style>
<div class="page-width hp-footer-main">
  <div class="hp-footer-grid">
    {% for block in section.blocks %}
      {% case block.type %}
        {% when 'feature' %}
        <style>
              .block-{{ block.id }} .footer-description p{
                font-size:{{ block.settings.desktop_body_size }}px;
              }
              .block-{{ block.id }} .footer-affiliate{
              font-size: {{ block.settings.desktop_bottom_size }}px;
              }
              @media screen and (max-width:756px){
                 .block-{{ block.id }} .footer-description p{
                font-size:{{ block.settings.mobile_body_size }}px;
              }
                .block-{{ block.id }} .footer-affiliate {
              font-size:{{ block.settings.mobile_bottom_size }}px;
              }
              }
            </style>
          <div class="footer-box block-{{ block.id }}">
            <div class="footer-logo">
              <a href="/">
            {% render 'icon-logo', fill: 'white' %}
              </a>
            </div>
            <div class="footer-description foot-tt">
              {{ block.settings.body }}
            </div>
            {% if block.settings.show_reviews %}
              <div class="stars">
                <div>
                  {% render 'google-stars' %}
                </div>
                <a href="{{ block.settings.link }}">{{ block.settings.link_text }}</a>
              </div>
            {% endif %}
            {% if block.settings.bottom_text != blank %}
              <p class="footer-affiliate">{{ block.settings.bottom_text }}</p>
            {% endif %}
             <!-- {%- render 'social-icons', class: 'footer__list-social' -%} -->
          </div>
        {% when 'links' %}
          <div class="footer-box footer-menu-box block-{{ block.id }}">
            <style>
              .block-{{ block.id }} h3{
                font-size:{{ block.settings.desktop_menu_heading_size }}px;
              }
              .block-{{ block.id }} li a {
              font-size: {{ block.settings.desktop_link_size }}px;
              }
              @media screen and (max-width:756px){
                 .block-{{ block.id }} h3{
                font-size:{{ block.settings.mobile_menu_heading_size }}px;
              }
                .block-{{ block.id }} li a {
              font-size:{{ block.settings.mobile_link_size }}px;
              }
              }
            </style>
            {% if block.settings.heading != blank %}
              <h3>
                <b>{{ block.settings.heading }}</b>
              </h3>
            {% endif %}
            <ul class="footer-menu foot-tt">
              {% for link in linklists[block.settings.menu_link].links %}
                <li class="text-base">
                  <a href="{{ link.url }}">{{ link.title }}</a>
                </li>
              {% endfor %}
            </ul>
          </div>
        {% when 'contact' %}
        <style>
              .block-{{ block.id }} h3{
                font-size:{{ block.settings.desktop_heading_size }}px;
              }
              .block-{{ block.id }} p{
              font-size: {{ block.settings.desktop_body_size }}px;
              }
              @media screen and (max-width:756px){
                 .block-{{ block.id }} h3{
                font-size:{{ block.settings.mobile_heading_size }}px;
              }
                .block-{{ block.id }} p {
              font-size:{{ block.settings.mobile_body_size }}px;
              }
              }
            </style>
          <div class="footer-box footer-text block-{{ block.id }}">
            {% if block.settings.heading != blank %}
              <h3>
                <b>{{ block.settings.heading }}</b>
              </h3>
            {% endif %}
            {{ block.settings.address }}
            <p>
              Phone: <a style="color: white;" href="tel:1-{{ block.settings.phone }}">{{ block.settings.phone }}</a>
            </p>
            <p>
              Email: <a style="color: white;" href="mailto:{{ block.settings.email }}">{{ block.settings.email }}</a>
            </p>

<ul class="list-unstyled list-social footer__list-social" role="list"><li class="list-social__item">
