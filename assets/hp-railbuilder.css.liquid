.box {
  position: relative;
}

#railImages .stack {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: auto;
}
#contact-us{
  display: block;
  text-align: center;
  width: 100%;
    border-width: 1px;
    border-style: none;
    background-color: var(--color-secondary);
    padding: 1rem 2rem;
    font-weight: 700;
    text-transform: none;
    --tw-text-opacity: 1;
    color: rgba(255, 255, 255, var(--tw-text-opacity));
    transition-property: all;
    transition-duration: .15s;
    transition-timing-function: cubic-bezier(.4,0,.2,1);
}